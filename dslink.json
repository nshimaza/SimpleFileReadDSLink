{
  "name": "dslink-scala-simple-file-read",
  "version": "0.0.1",
  "description": "Create DSLink Node, read a file and set its content to the Node then exit",
  "license": "Cisco Sample Code License",
  "author": {
    "name": "Naoto Shimazaki",
    "email": "nshimaza@cisco.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nshimaza/dslink-scala-simple-file-read"
  },
  "bugs": {
    "url": "https://github.com/nshimaza/dslink-scala-simple-file-read/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "SimpleFileRead"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "com.example.SimpleFileReadDSLinkHandler"
    }
  }
}
